<Planet>:
	#:import random random
	size_hint: None, None
	canvas:
		Color:
			rgba: random.random(), random.random(), random.random(), 1
		Ellipse:
			size: self.width, self.height
			pos: self.pos

<MainScreen>:
	canvas:
		Color:
			rgba: 1., 1., 1., 1
		Rectangle:
			size: self.width, self.height
